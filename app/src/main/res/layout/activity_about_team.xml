<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".About_Code_For_Iraq">

    <ImageView
            android:layout_width="180dp"
            android:background="@drawable/shadow"
            android:layout_marginTop="30dp"

            android:layout_height="180dp" app:srcCompat="@drawable/logo" android:id="@+id/imageView4"
            android:layout_gravity="center_horizontal"/>
    <TextView
            android:text="البرمجة من اجل العراق"
            android:layout_width="wrap_content"
            android:layout_marginTop="250dp"
            android:layout_height="wrap_content" android:id="@+id/فtextView8" android:layout_gravity="center_horizontal"
            android:textSize="18sp"/>
    <TextView
            android:text="فريق صلاح الدين"
            android:layout_width="wrap_content"
            android:layout_marginTop="300dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:layout_gravity="center_horizontal"
            android:textSize="18sp"/>
    <FrameLayout
            android:layout_width="350dp"
            android:layout_height="400dp" android:layout_gravity="bottom|center_horizontal">

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
            <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp" app:srcCompat="@drawable/mahmod" android:id="@+id/imageView5"/>
            <TextView
                    android:text="Mahmod Nory"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView6"
                    android:layout_gravity="center" android:textAlignment="center"/>
            <TextView
                    android:text="مبرمج المشروع"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView13" android:textAlignment="center"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_gravity="right">
            <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp" app:srcCompat="@drawable/muhammedessa"
                    android:id="@+id/imageView6"/>
            <TextView
                    android:text="Muhammed Essa"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView9" android:textAlignment="center"/>
            <TextView
                    android:text="مدير المشروع"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView12" android:textAlignment="center"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_gravity="bottom|right"
                android:layout_marginBottom="100dp" android:layout_marginRight="50dp">
            <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp" app:srcCompat="@drawable/marwan" android:id="@+id/imageView7"/>
            <TextView
                    android:text="Marwan Ahmed"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView10" android:textAlignment="center"/>
            <TextView
                    android:text="مصمم المشروع"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView14" android:textAlignment="center"/>
        </LinearLayout>
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_gravity="bottom"
                android:gravity="center" android:layout_marginBottom="100dp" android:layout_marginLeft="50dp">
            <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp" app:srcCompat="@drawable/osama" android:id="@+id/imageView8"/>
            <TextView
                    android:text="Osama Abdulsatar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView11" android:textAlignment="center"/>
            <TextView
                    android:text="خوارزمية المشروع"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView15" android:textAlignment="center"/>
        </LinearLayout>
    </FrameLayout>


</FrameLayout>